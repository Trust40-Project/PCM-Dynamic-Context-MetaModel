<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
>
   <extension
         point="org.eclipse.debug.core.launchConfigurationTypes">
      <launchConfigurationType
            delegate="org.palladiosimulator.pcm.confidentiality.context.attackanalysis.launcher.delegate.LaunchDelegate"
            id="pcm.dataprocessing.context.attackeranalysis.launcher.config"
            modes="run"
            name="Attackeranalysis"
            public="true">
      </launchConfigurationType>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTypeImages">
      <launchConfigurationTypeImage
            configTypeID="pcm.dataprocessing.context.attackeranalysis.launcher.config"
            icon="icons/lock-24px.svg"
            id="pcm.dataprocessing.context.attackeranalysis.analysisicon">
      </launchConfigurationTypeImage>
   </extension>
   <extension
         point="org.eclipse.debug.ui.launchConfigurationTabGroups">
      <launchConfigurationTabGroup
            class="org.palladiosimulator.pcm.confidentiality.context.attackanalysis.launcher.ui.LaunchConfigurationTabGroup"
            id="pcm.dataprocessing.context.attackeranalysis.launcher.tabGroup"
            type="pcm.dataprocessing.context.attackeranalysis.launcher.config">
         <launchMode
               mode="run"
               perspective="org.eclipse.debug.ui.DebugPerspective">
         </launchMode>
      </launchConfigurationTabGroup>
   </extension>

   
  
</plugin>
